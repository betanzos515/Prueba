import UIKit

var rango = 0...100

for numero in rango {
    if numero % 10 == 5
    {
        print ("# el numero \(numero) Bingo!!!")
    }
    else if numero == 0{
        print("# el numero \(numero) Bingo!!!")
    }
    if numero % 2 == 0
    {
    print("# El numero \(numero) par!!")
    }
    else
    {
     print("# El numero \(numero) impar!!")
    }
 
    if(numero>=30){
        if(numero<=40 )
        {
            print("# el numero \(numero) viva suit")
    }
    }
}
